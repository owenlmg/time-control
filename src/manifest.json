{
  "name": "time-control",
  "description": "__MSG_appDescription__",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "__MSG_defaultTitle__",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options/options.html",
  "default_locale": "en",
  "permissions":
  [
    "notifications",
    "storage",
    "activeTab"
  ]
}
